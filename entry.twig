{% extends "_base.twig" %}
{% block main %}
{{print(record)}}
<h5>{{record.datepublished|date('F j, Y')}} <small>in {% for cat in record.taxonomy.categories %}{{cat}}{% endfor %}</small></h5>
{% if record.image %}
<img src="{{record.image|thumbnail(320,240)}}" alt="" />
{% endif %}
{{record.body}}
{% endblock %}